<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Favorite_Board">
	<insert id="insertFav" useGeneratedKeys="true" keyProperty="seq">
		insert into Favorite_Board values (null,#{board_title},#{id},#{parent_seq})
	</insert>

	<delete id="delFavContents">
		delete from Favorite_Board where seq=${seq}
	</delete>
</mapper>